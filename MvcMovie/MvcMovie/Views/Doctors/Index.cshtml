@model IEnumerable<MvcMovie.Models.Doctors>

@{ 
    Layout = "../Shared/_Layout.cshtml";
}

<h1>Lista lekarzy</h1>

<table>
    <thead>
        <tr>
            <th>@Html.DisplayNameFor(modelItem => modelItem.FirstName)</th>
            <th>@Html.DisplayNameFor(modelItem => modelItem.LastName)</th>
            <th>@Html.DisplayNameFor(modelItem => modelItem.Email)</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var d  in Model)
        {
            <tr>
                <td>@d.FirstName</td>
                <td>@d.LastName</td>
                <td>@d.Email</td>
            </tr>
        }
    </tbody>
</table>

